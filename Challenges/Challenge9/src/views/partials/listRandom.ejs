<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Foto</th>
        </tr>
    </thead>
    <tbody id="table-body">
        <% products.forEach(product=>{ %>
            <tr>
                <td class="table-info"><%= product.title %></td>
                <td class="table-info"><%= product.price %></td>
                <td class="table-info"><img src=<%= product.image %> alt="...."></td>
            </tr>
        <% }) %>
    </tbody>
</table>